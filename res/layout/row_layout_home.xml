    -->

    <RelativeLayout
        android:id="@+id/home_rowlayout"
        android:layout_width="match_parent"
        android:layout_height="124dip"
        android:background="#AAE0E2" >

        <TextView
            android:id="@+id/title_home"
            style="@style/feedTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginBottom="10dip"
            android:layout_marginLeft="12dip"
            android:layout_marginRight="80dip"
            android:layout_marginTop="5dip" />

        <TextView
            android:id="@+id/start_text_home"
            style="@style/homeDate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/schedule_text_home"
            android:layout_alignLeft="@id/title_home"
            android:text="@string/started" />

        <TextView
            android:id="@+id/start_home"
            style="@style/homeDate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/start_text_home"
            android:layout_marginLeft="45dip"
            android:layout_toRightOf="@id/start_text_home" />

        <TextView
            android:id="@id/schedule_text_home"
            style="@style/homeDate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@id/title_home"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="3dip"
            android:layout_marginTop="2dip" />

        <TextView
            android:id="@+id/schedule_home"
            style="@style/homeDate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/schedule_text_home"
            android:layout_alignLeft="@id/start_home" />

        <ImageView
            android:id="@+id/icon_home"
            android:layout_width="75dip"
            android:layout_height="85dip"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_margin="3dip"
            android:scaleType="centerInside" />
    </RelativeLayout>

    <View
        android:id="@+id/home_grey_mask"
        android:layout_width="match_parent"
        android:layout_height="124dip"
        android:background="@drawable/listitem_selector" />

    <View
        android:id="@+id/home_shadow"
        android:layout_width="match_parent"
        android:layout_height="6dip"
        android:layout_below="@id/home_rowlayout"
        android:background="@drawable/shadow" />

</RelativeLayout>
